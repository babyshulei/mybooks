{
    "plugins": [
        "chapter-fold",
        "splitter",
        "toc2",
        "git-author"
    ],
    "pluginsConfig": {
        "toc2": {
            "addClass": true,
            "className": "toc"
        },
        "git-author": {
            "modifyTpl": "<span style=\"font-size:75%\">Last modified by {user} {timeStamp}</span>",
            "createTpl": "<span style=\"font-size:75%\">Created by {user} {timeStamp}</span>"
        }
    }
}

